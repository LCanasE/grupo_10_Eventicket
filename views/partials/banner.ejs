<div class="gallery-container">
    <% eventos.forEach(evento => { %>
      <img src= "/../<%= evento.img %>" alt= "<% evento.name %>" class="gallery-item">
    <% }); %>
    <div class="gallery-btn">
      <button id="gallery-btn-left"><i class="fa-solid fa-arrow-left"></i></button>
      <button id="gallery-btn-right"><i class="fa-solid fa-arrow-right"></i></button>
    </div>
  </div>